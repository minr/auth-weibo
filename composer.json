{
    "name": "minr/flarum-ext-auth-weibo",
    "description": "Allow users to log in with Weibo.",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "flarum/core": "^0.1.0-beta.10",
        "league/oauth2-client": "^2.4",
        "ext-json": "*"
    },
    "version": "v0.1.4",
    "license": "MIT",
    "authors": [
        {
            "name": "min",
            "email": "min@blued.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Minr\\Auth\\Weibo\\": "src/"
        }
    },
    "extra": {
