{
    "name": "minr/flarum-ext-auth-weibo",
    "description": "Allow users to log in with Weibo.",
    "type": "flarum-extension",
    "keywords": [
        "flarum",
        "authentication",
        "weibo"
    ],
    "license": "MIT",
    "version": "v0.1.9",
    "authors": [
        {
            "name": "MIN",
            "email": "294953530@qq.com",
            "role": "master"
        }
    ],
    "require": {
        "flarum/core": "^1.0.0",
        "league/oauth2-client": "^2.2",
        "laravel/helpers": "^v1.4.1",
        "fof/components": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Minr\\Auth\\Weibo\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Weibo Login",
            "icon": {
                "name": "fab fa-weibo",
